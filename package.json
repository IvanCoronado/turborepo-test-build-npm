{
  "name": "turborepo-test-build",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build --cache-dir=.turbo",
    "deploy": "turbo run deploy --cache-dir=.turbo",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "vercel-build": "npx vercel pull --yes --environment=production && npx vercel build --prod",
    "vercel-deploy": "npx vercel deploy --prod --prebuilt"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "npm@8.3.2"
}
